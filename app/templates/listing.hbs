<h1>{{model.title}} </h1>
{{#if session.isAuthenticated}}
  <button class='btn btn-danger btn-xs' {{action 'deleteListing' model}}>delete</button>
  {{listing-update listing=model editListing='editListing' class='button-inline'}}
{{/if}}
<p>
  created: {{moment-from-now model.createdAt}}
</p>
<p class='content'>
  {{model.content}}
</p>
{{outlet}}
